<%= form_with model: @item, html: {multipart: true}, local: true do |form| %>

<p>
  <%= form.label :name %> <br>
  <%= form.text_field :name %>
</p>

 <%= form.file_field :image%>

<p>
  <%= form.label :description %> <br>
  <%= form.text_field :description %>
</p>

<p>
  <%= form.submit %>
</p>

<% end %>
